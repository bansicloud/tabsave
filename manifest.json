{
  "name": "Tablist",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "a browser extension to copy and paste lists of open tabs",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' https://unpkg.com https://cdnjs.cloudflare.com; object-src 'self'"
}
